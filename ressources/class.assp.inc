	private function load(){
		$sock=new sockets();
		$datas=trim($sock->GET_INFO('ASSPConfig'));
		if(trim($datas==null)){$datas=$sock->getFrameWork("cmd.php?ASSPOriginalConf");}
		$tbl=explode("\n",$datas);
		while (list ($num, $ligne) = each ($tbl) ){
			if(preg_match("#(.+?):=(.*)#",$ligne,$re)){
				$key=trim($re[1]);
				$this->$key=trim($re[2]);
			}
		}
		
	}
	
